@article{Chen2014,
abstract = {Activity models play a critical role for activity recognition and assistance in ambient assisted living. Existing approaches to activity modeling suffer from a number of problems, e.g. cold-start, model reusability and incompleteness. In an effort to address these problems, we introduce an ontology- based hybrid approach to activity modeling that combines domain knowledge based model specification and data-driven model learning. Central to the approach is an iterative process that begins with “seed” activity models created by ontological engineering. The “seed” models are deployed, and subsequently evolved through incremental activity discovery and model update. While our previous work has detailed ontological activity modeling and activity recognition, this paper focuses on the systematic hybrid approach and associated methods and inference rules for learning new activities and user activity profiles. The approach has been implemented in a feature-rich assistive living system. Analysis of the experiments conducted has been undertaken in an effort to test and evaluate the activity learning algorithms and associated mechanisms.},
annote = {        Summary:
                
Authors present a hybrid model that combines data-driven approaches and ontological based activity models. They argue that knowledge-driven activity models are static and lack of the specific ways a person executes activities. To tackle the problem, they propose a three step iterative process.
        
1) Create the ontological-based activity recognition system manually, where seed activity models are developed
        
2) Collect sensor data (action traces) and classify them using the recognition system -> generate 2 groups: recognised activities (LATs) and unrecognised activities (UATs)
        
3) Learn user profiles with LATs and new activity models with UATs
        
User profiles have three main fields: objetc pattern, duration and activity pattern. They propose some techniques based on semantic similarity measures and frequency analysis to learn those three aspects of an activity model.
        
Additionally, authors also show a method to learn new activities from UATs. Using a semantic similarity measure they cluster UATs. Each cluster represents a new activity that has to be labeled and manually introduced to the ontologic activity models.
                  
Terms and notation
        Activity = ontological concept
Activity is defined by action properties (hasTeabag, hasFlavour...)
Activities also have descriptive properties (actId, actStartTime, actDuration...)
Action properties play a crucial role in activity recognition
Descriptive properties, on the other hand, are not determinants in activity recognition
Action trace = sequence of sensor observations within a time segment},
author = {Chen, Liming and Nugent, Chris and Okeyo, George},
file = {:home/gazkune/.local/share/data/Mendeley Ltd./Mendeley Desktop/Downloaded/Chen, Nugent, Okeyo - 2014 - An Ontology-based Hybrid Approach to Activity Modeling for Smart Homes.pdf:pdf},
journal = {IEEE Transaction on Human-Machine Systems},
keywords = {activity model learning,activity recognition,ontology,semantic reasoning,smart homes},
number = {1},
pages = {92--105},
title = {{An Ontology-based Hybrid Approach to Activity Modeling for Smart Homes}},
volume = {44},
year = {2014}
}
@article{Linda2010,
abstract = {Dynamic uncertainties, manifested as input noise or variable environment conditions, are an inherent part of most real world control applications. Recently, several researchers demonstrated that Type-2 Fuzzy Logic Controllers (T2 FLC) are able to cope with such uncertainty and reduce its negative effects. However, the design and optimization of T2 FLC and its subsequent unbiased comparison to T1 FLC are still an open question. This paper presents a comparative analysis of interval T2 (IT2) and T1 FLCs in the context of learning behaviors for mobile robotics. First, a T1 FLC is optimized using the Particle Swarm Optimization algorithm to mimic a wall-following behavior performed by an operator. Next, an IT2 FLC is constructed by symmetrically blurring the fuzzy sets of the original T1 FLC. The performance of the fuzzy controllers is compared using a wall-following sonar-equipped mobile robot in both noise-free and noisy environments. It is experimentally demonstrated that the IT2 FLC can cope better with dynamic uncertainties in the sensory inputs due to the softening and smoothing of the output control surface by the IT2 fuzzy sets. However, the IT2 FLC is outperformed by the T1 FLC when sudden and fast response of the controller is required, such as in the case of turning around corners. Those results suggest the difficulties of symmetrical blurring of T1 FLC (although commonly used) as a design methodology for obtaining the architecture of an IT2 FLC.},
author = {Linda, Ondrej and Manic, Milos},
file = {:home/gazkune/.local/share/data/Mendeley Ltd./Mendeley Desktop/Downloaded/Linda, Manic - 2010 - Comparative analysis of type-1 and type-2 fuzzy control in context of learning behaviors for mobile robotics.pdf:pdf},
isbn = {9781424452262},
journal = {IECON 2010-36th Annual Conference on IEEE Industrial Electronics Society},
pages = {1086--1092},
title = {{Comparative analysis of type-1 and type-2 fuzzy control in context of learning behaviors for mobile robotics}},
url = {http://ieeexplore.ieee.org/xpls/abs\_all.jsp?arnumber=5675521},
year = {2010}

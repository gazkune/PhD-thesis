@article{Riboni2011a,
abstract = {Human activity recognition is a challenging problem for context-aware systems and applications. Research in this field hasmainly adopted techniques based on supervised learning algorithms, but these systemssuffer from scalability issues with respect to the number of considered activities and contextual data. In this paper, we propose a solution based on the use of ontologies and ontological rea- soning combined with statistical inferencing. Structured symbolic knowledge about the environment surrounding the user allows the recognition system to infer which activities among the candidates identified by statistical methods are more likely to be the actual activity that the user is per- forming. Ontological reasoning is also integrated with sta- tistical methods to recognize complex activities that cannot be derived by statistical methods alone. The effectiveness of the proposed technique is supported by experiments with a complete implementation of the system using commercially available sensors and an Android-based handheld device as the host for the main activity recognition module.},
annote = {Summary:
        
Authors present an approach to combine data-driven approaches with ontological reasoning for activity recognition. However, the approach is quite simple. They use data-driven approaches to train models that use sensor data (mainly IMUs) for activity recognition. On the other hand, they use an ontology to describe the activities and mainly their relationships with locations. 
        
They present an algorithm (DPA) that using the definitions of activities and locations, infers which activities can be executed in which locations, usign consistency checks of the ontology. Using the result, they evaluate the ouptut of the data-drien system in the following way:
        
1) Take the most probable activity
2) Check whether the activity can be performed in the location reported by the localization system
3) If Yes, the actiity has been recognised
4) If Not, discard the prediction and go back to step 1
        
As future work they identify the extension of the ontology to include also temporal information (duration, sequence of activities...).},
author = {Riboni, D and Bettini, C},
file = {:home/gazkune/.local/share/data/Mendeley Ltd./Mendeley Desktop/Downloaded/Riboni, Bettini - 2011 - COSAR hybrid reasoning for context-aware activity recognition.pdf:pdf},
journal = {Personal and Ubiquitous Computing},
keywords = {Activity recognition,Context awareness,Ontological reasoning},
mendeley-tags = {Activity recognition,Context awareness,Ontological reasoning},
number = {3},
pages = {271--289},
title = {{COSAR: hybrid reasoning for context-aware activity recognition}},
url = {http://link.springer.com/article/10.1007/s00779-010-0331-7},
volume = {15},
year = {2011}
}

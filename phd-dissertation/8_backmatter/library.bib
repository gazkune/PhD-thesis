@article{Riboni2011b,
abstract = {In recent years, there has been a growing interest in the adoption of ontologies and ontological reasoning to automatically recognize complex context data such as human activities. In particular, the Web Ontology Language (OWL) emerged as the language of choice, being a standard for the Semantic Web, and supported by a number of tools for knowledge engineering and reasoning. However, the limitations of OWL 1 in terms of expressiveness have been recognized in various fields, and important research efforts have been made to extend the language while preserving decidability of its OWL 1 DL fragment. The result of such work isOWL2. In this paperweinvestigate the use ofOWL2 for modeling complex activities and reasoning with them.Weshow that the new language constructors of OWL 2 overcome the main limitations of OWL 1 for the representation of activities; OWL 2 axioms can be used to represent certain rules and rule-based reasoning previously demanded to hybrid approaches, with the advantage of having a unique semantics, avoiding potential inconsistencies. Then, we propose a system architecture showing the integration of a novel OWL 2 activity ontology and reasoning modules with distributed modules for sensor data aggregation and reasoning. The feasibility of our solution is shown by an extensive experimental evaluation with simulations of different intelligent environments.},
annote = {        Summary:        
        
This paper makes an extensive feature description of OWL2 and shows how it can be used for complex activity recognition. It argues that OWL2 solves many of the problems found in OWL1, which were encouraging the adoption of hybrid techniques (OWL+rules). They argue that OWL2 allows avoiding rule-based extensions, which are problematic because they make the whole reasoning set undecidible. To show this, they provide a comparison among common hybrid approaches and OWL2. 
        
They show a possible solution for activity recognition based on ontologic reasoning, providing some experiments. Finally, they point out that data uncertainty cannot be taken into account natively in OWL2. They propose this as future work and point out some early solutions.},
author = {Riboni, D and Bettini, C},
file = {:home/gazkune/.local/share/data/Mendeley Ltd./Mendeley Desktop/Downloaded/Riboni, Bettini - 2011 - OWL 2 modeling and reasoning with complex human activities.pdf:pdf},
journal = {Pervasive and Mobile Computing},
keywords = {Activity recognition,Context-awareness,Ontological reasoning},
number = {3},
pages = {379--395},
title = {{OWL 2 modeling and reasoning with complex human activities}},
url = {http://www.sciencedirect.com/science/article/pii/S1574119211000265},
volume = {7},
year = {2011}
}

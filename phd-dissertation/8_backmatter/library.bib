@inproceedings{Riboni2011,
abstract = {While most activity recognition systems rely on data-driven approaches, the use of knowledge-driven tech- niques is gaining increasing interest. Research in this field has mainly concentrated on the use of ontologies to specify the semantics of activities, and ontological reasoning to recognize them based on context information. However, at the time of writing, the experimental evaluation of these techniques is limited to computational aspects; their actual effectiveness is still unknown. As a first step to fill this gap, in this paper, we experimentally evaluate the effectiveness of the ontological approach, using an activity dataset collected in a smart-home setting. Preliminary results suggest that existing ontological techniques underperform data-driven ones, mainly because they lack support for reasoning with temporal infor- mation. Indeed, we show that, when ontological techniques are extended with even simple forms of temporal reasoning, their effectiveness is comparable to the one of a state-of- the-art technique based on Hidden Markov Models. Then, we indicate possible research directions to further improve the effectiveness of ontology-based activity recognition through temporal reasoning.},
annote = {        Summary:        
        
This paper provides an interesting comparison of ontology-based VS data-driven activity recognition. They point out the importance of temporal reasoning for acitivity recognition, which data-driven approaches handle, but not the ontological ones. They propose some wats to handle temporal reasoning in the ontological approach and show the results may outperform data-driven approaches. Finally, they show future lines for incorporating complex temporal reasoning in ontologic frameworks
                  
Comments:
          
        No uncertainty is considered in the sensors, since they are "simple" sensors.},
author = {Riboni, D and Pareschi, L},
booktitle = {Pervasive Computing and Communications Workshops (PERCOM Workshops), IEEE International Conference on},
file = {:home/gazkune/.local/share/data/Mendeley Ltd./Mendeley Desktop/Downloaded/Riboni, Pareschi - 2011 - Is ontology-based activity recognition really effective.pdf:pdf},
title = {{Is ontology-based activity recognition really effective?}},
url = {http://ieeexplore.ieee.org/xpls/abs\_all.jsp?arnumber=5766927},
year = {2011}
}
